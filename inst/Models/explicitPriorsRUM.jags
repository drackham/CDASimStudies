model
{
  for(i in 1:I){  #40
    for(j in 1:J){ #1500
      for(k in 1:K){
        d[i,j,k] <- rStar[i,k]^((1-alpha[j,k])*q[i,k])
      }
    }
  }

  for(i in 1:I){
    for(j in 1:J){
      x[i,j] ~ dbern(prod(d[i,j,]))
    }
  }

  for (i in 1:I){
    for (k in 1:K){
      ifelse (q[i,k]==1, rStar[i,k] ~ dunif(1,1), rStar[i,k] ~ dbeta(2,8))
    }
  }


  for(j in 1:J){
    for(k in 1:K){
      alpha[j,k] ~ dbeta(3,3)
    }
  }

}

